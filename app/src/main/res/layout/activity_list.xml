<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/appbar"
        android:layout_height="192dp"
        android:layout_width="match_parent">

        <android.support.design.widget.CollapsingToolbarLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:toolbarId="@+id/toolbar"
            app:layout_scrollFlags="scroll|enterAlways|enterAlwaysCollapsed"
            app:contentScrim="?attr/colorPrimary"
            android:animateLayoutChanges="true">

            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar1"
                android:layout_height="?attr/actionBarSize"
                android:layout_width="match_parent">
            </android.support.v7.widget.Toolbar>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="138dp"
                app:srcCompat="@mipmap/listicon"
                android:id="@+id/imageView2" />

        </android.support.design.widget.CollapsingToolbarLayout>
    </android.support.design.widget.AppBarLayout>

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="android.support.design.widget.AppBarLayout$ScrollingViewBehavior">
        <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/activity_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="@dimen/activity_vertical_margin"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:paddingTop="@dimen/activity_vertical_margin"
            tools:context="com.example.amoghagnihotri.amoghapp.list">

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:freezesText="true"
                android:ems="10"
                android:layout_below="@+id/editText4"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:id="@+id/firstet"/>

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:layout_below="@+id/firstet"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:id="@+id/secondet" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:layout_below="@+id/secondet"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:id="@+id/thirdet"/>

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:layout_below="@+id/thirdet"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:id="@+id/fourthet" />


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:layout_below="@+id/fourthet"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:id="@+id/fifthet"/>

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:layout_below="@+id/fifthet"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:id="@+id/sixthet"/>

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:layout_below="@+id/sixthet"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:id="@+id/seventhet"/>

            <CheckBox
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/fourthbox"
                android:onClick="checked"
                android:layout_below="@+id/thirdet"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:tag="et4"/>

            <CheckBox
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/fifthbox"
                android:onClick="checked"
                android:layout_below="@+id/fourthet"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:tag="et5"/>

            <CheckBox
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/thirdbox"
                android:tag="et3"
                android:onClick="checked"
                android:layout_alignBaseline="@+id/but3"
                android:layout_alignBottom="@+id/but3"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <CheckBox
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/firstbox"
                android:onClick="checked"
                android:tag="et1"
                android:layout_alignBaseline="@+id/but1"
                android:layout_alignBottom="@+id/but1"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <CheckBox
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/secondbox"
                android:tag="et2"
                android:onClick="checked"
                android:layout_alignBaseline="@+id/but2"
                android:layout_alignBottom="@+id/but2"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
            <CheckBox
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/seventhbox"
                android:tag="et7"
                android:onClick="checked"
                android:layout_below="@+id/but6"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <CheckBox
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/sixthbox"
                android:tag="et6"
                android:onClick="checked"
                android:layout_alignBaseline="@+id/but6"
                android:layout_alignBottom="@+id/but6"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <Button
                android:text="Add Item"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/but1"
                android:tag="but1"
                android:onClick="added"
                android:layout_above="@+id/secondet"
                android:layout_toLeftOf="@+id/firstbox"
                android:layout_toStartOf="@+id/firstbox" />

            <Button
                android:text="Add Item"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/but2"
                android:tag="but2"
                android:onClick="added"
                android:layout_above="@+id/thirdet"
                android:layout_alignLeft="@+id/but1"
                android:layout_alignStart="@+id/but1" />

            <Button
                android:text="Add Item"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/but3"
                android:tag="but3"
                android:onClick="added"
                android:layout_above="@+id/fourthet"
                android:layout_alignLeft="@+id/but2"
                android:layout_alignStart="@+id/but2" />

            <Button
                android:text="Add Item"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/but4"
                android:tag="but4"
                android:onClick="added"
                android:layout_above="@+id/fifthet"
                android:layout_alignLeft="@+id/but3"
                android:layout_alignStart="@+id/but3" />

            <Button
                android:text="Add Item"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/but5"
                android:tag="but5"
                android:onClick="added"
                android:layout_above="@+id/sixthet"
                android:layout_alignLeft="@+id/but4"
                android:layout_alignStart="@+id/but4" />

            <Button
                android:text="Add Item"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/but6"
                android:tag="but6"
                android:onClick="added"
                android:layout_alignBaseline="@+id/sixthet"
                android:layout_alignBottom="@+id/sixthet"
                android:layout_alignLeft="@+id/but5"
                android:layout_alignStart="@+id/but5" />

            <Button
                android:text="Add Item"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/but7"
                android:tag="but7"
                android:onClick="added"
                android:layout_below="@+id/sixthet"
                android:layout_alignLeft="@+id/but6"
                android:layout_alignStart="@+id/but6" />
        </RelativeLayout>
    </android.support.v4.widget.NestedScrollView>
</android.support.design.widget.CoordinatorLayout>




